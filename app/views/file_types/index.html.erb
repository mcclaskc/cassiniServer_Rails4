<h1>Listing file_types</h1>
<%= link_to 'New File type', new_file_type_path %>
</br></br>

<%= will_paginate @file_types %>
<table cellpadding="10">
  <thead>
    <tr>
      <th>Id</th>
      <th>Title</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @file_types.each do |file_type| %>
    <tr>
      <td><%= file_type.id %></td>
      <td><%= file_type.title %></td>
      <td><%= link_to 'Show', file_type, :class => 'btn' %></td>
      <td><%= link_to 'Edit', edit_file_type_path(file_type), :class => 'btn' %></td>
      <td><%= link_to 'Destroy', file_type, method: :delete, data: { confirm: 'Are you sure?' }, :class => 'btn' %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<br />

